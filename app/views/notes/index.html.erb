    <header class='clearfix'>
        <ul>
          <li><%= link_to image_tag("ch_delta_red_logo_sm.png"), "https://www.changehealthcare.com/" %></li>
          <li><a href="https://www.changehealthcare.com/">CHC</a></li>
          <li><%= link_to "Worklist", root_path %></li>
  		  <li><%= link_to "About", static_pages_about_path %></li>
          <li><%= link_to "Create Note", new_encounter_note_path(@encounter_id) %></li>
        </ul>
    </header>

<div class='content'>
<h1>Encounter Notes for <%= @encounter.patient_account_number %></h1>
<h3>Show Notes Page</h3>

<table id="encounters">
	<tbody>
		<tr>
			<th>Note ID</th>
			<th>Internal Patient ID</th>
			<th>PAN</th>
			<th>Note Content</th>
			<th>Staff Initials</th>
			<th>Create Date</th>
			<th>Update Date</th>
			<th>Edit Note</th>
			<th>Delete Note</th>
		 <% @notes.each do |note| %>		 
		<tr>
			<td><%= note.id %></td>
			<td><%= note.encounter_id %></td>
			<td><%= note.patient_account_number %></td>
			<td><%= note.content %></td>
			<td><%= note.staff_initials %></td>
			<td><%= note.created_at %></td>
			<td><%= note.updated_at %></td>
			<td><%= link_to "Edit Note", edit_encounter_note_path(@encounter_id, note)%></td>
			<td><%= link_to 'Delete Note', encounter_note_path(@encounter_id, note), method: :delete %></td>
		</tr>
		<% end %>

	</tbody>
</table>

<br>
<b>Disclaimer:</b> Fake Data
<br>

<%= link_to("Create note", new_encounter_note_path(@encounter_id)) %>
<br>
<%= link_to 'Go Back to Patient Encounter List', encounters_path, method: :get %>
