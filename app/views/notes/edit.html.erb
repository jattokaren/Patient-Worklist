    <header class='clearfix'>
        <ul>
          <li><%= link_to image_tag("ch_delta_red_logo_sm.png"), "https://www.changehealthcare.com/" %></li>
          <li><%= link_to "About", about_path %></li>
          <li><%= link_to "Worklist", root_path %></li>
          
          <li><%= link_to "Back to Notes", encounter_notes_path(params[:encounter_id])%></li>
        </ul>
    </header>

<div class='content'>    
<h1>Edit Note for Encounter:</h1>

<%= form_for @note, url: encounter_note_path(@encounter_id, @note) do |f| %>
  <div>
    <b><%= f.label :content %></b>
    <br>
    <%= f.text_area :content, size: '50x5' %>
  </div>
  <div>
    <b><%= f.label :staff_initials %></b>
    <br>
    <%= f.text_field :staff_initials %>
  </div>
  <br>
  <div>
    <%= f.submit %>
  </div>
<% end %>

<br>
<%= link_to "Back to Encounter Notes", encounter_notes_path %>
</div>